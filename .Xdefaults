URxvt*termName: xterm-color
URxvt.scrollBar: false

URxvt*font: xft: SourceCodePro:size=10

Xft.font: xft: SourceCodePro:size=10


URxvt.depth: 32

Xft.antialias: true
Xft.dpi: 96
Xft.hinting: true
Xft.hintstyle: hintfull
Xft.autohint: 0
Xft.rgba: rgb
Xft.lcdfilter: lcddefault

URxvt.perl-ext-common:  default,matcher,clipboard,keyboard-select
URxvt.urlLauncher:      /usr/bin/chromium
URxvt.matcher.button:   1

URxvt.url-select.launcher:  /usr/bin/chromium
URxvt.url-select.underline: true
URxvt.clipboard.autocopy: 

URxvt.clipboard.copycmd:  xclip -i -selection clipboard
URxvt.clipboard.pastecmd: xclip -o -selection clipboard



! Xresources
#URxvt*background:	#000C1A
URxvt*background:       #000000
URxvt*foreground:	#ACB4BF
URxvt*cursorColor:	#335A80

*background:		#CFCFD9
*foreground:		#1C2027

